#include<stdio.h>

int main()
{
    int n1,n2,lcm,f=1;
    
    printf("Enter 2 number ");
    scanf("%d%d",&n1,&n2);
    
    lcm  = (n1>n2) ? n1 :n2;
    
    while(f)
    {
        if(lcm%n1==0 && lcm%n2==0)
        {
            printf("lcm of a number is %d",lcm);
            f=0;
            
        }
        lcm++;
    }
   
}
