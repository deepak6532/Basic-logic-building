#include<stdio.h>

int main()
{
    int num,sum=0,r;
    
    
    printf("Enter a number ");
    scanf("%d",&num);
    int square = num * num;
    
    while(square>0)
    {
        r = square%10;
        sum = sum + r;
        square = square/10;
        
    }
    if(num == sum)
    {
        printf("Neon");
        
    }
    else
    {
        printf("Not");
    }
}
